<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <CompilerOptions>
    <Version Value="11"/>
    <Target>
      <Filename Value="RobotWin32"/>
    </Target>
    <SearchPaths>
      <SrcPath Value="$(LazarusDir)/lcl;$(LazarusDir)/lcl/interfaces/$(LCLWidgetType)"/>
    </SearchPaths>
    <Parsing>
      <SyntaxOptions>
        <UseAnsiStrings Value="False"/>
      </SyntaxOptions>
    </Parsing>
    <Linking>
      <Options>
        <Win32>
          <GraphicApplication Value="True"/>
        </Win32>
      </Options>
      <Debugging>
        <UseHeaptrc Value="True"/>
      </Debugging>
    </Linking>
    <Other>
      <Verbosity>
        <ShowExecInfo Value="True"/>
      </Verbosity>
      <CreateMakefileOnBuild Value="True"/>
    </Other>
  </CompilerOptions>
</CONFIG>
